FORM SELEZIONE_INIZIALE.
     CALL SELECTION_SCREEN 123.
ENDFORM.

FORM CONTROLLA_AGENTE.
     SELECT SINGLE * FROM KNVV WHERE KVNNR = KNA1-KUNNR AND BZIRK IN AGENTI.
ENDFORM.

FORM CONTROLLA_cOND_PAG.
     SELECT SINGLE * FROM KNVV WHERE KUNNR = KNA1-KUNNR AND ZTERN IN PAGAMENTO.
ENDFORM.

DYNPRO '200'.

PROCESS BEFORE OUTUPUT.
        MODULE LIST.
PROCESS AFTER INPUT.

MODULE LIST OUTPUT.
   DESCRIBE TABLE IT_TABVID1 LINES LINEE.
     IF LINEE <> 0.
        SORT IT_TABVID1 BY KUNNR.
        
             LOOP AT IT_TABVID1 INTO WA_TABVID.
                DO  DWA_TABVID-NUM TIMES.
                      WRITE: / WA_TABVID-KUNNR,
                               WA_TABVID-NAME1.
                ENDDO.
             ENDLOOP.
     ENDIF.
   SUPPRESS DIALOG LEAVE   TO LIST-PROCESSING
       AND RETURN TO SCREEN 0.
ENDMODULE.

